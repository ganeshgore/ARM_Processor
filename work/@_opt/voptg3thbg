library verilog;
use verilog.vl_types.all;
entity Counter2 is
    port(
        clk             : in     vl_logic;
        rst             : in     vl_logic;
        \IN\            : in     vl_logic_vector(1 downto 0);
        \Out\           : out    vl_logic_vector(1 downto 0)
    );
end Counter2;
