library verilog;
use verilog.vl_types.all;
entity CPSRREG is
    port(
        clk             : in     vl_logic;
        rst             : in     vl_logic;
        load            : in     vl_logic;
        \IN\            : in     vl_logic_vector(31 downto 0);
        Vin             : in     vl_logic;
        Cin             : in     vl_logic;
        \OUT\           : out    vl_logic_vector(31 downto 0)
    );
end CPSRREG;
